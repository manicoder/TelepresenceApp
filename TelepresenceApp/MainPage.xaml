<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TelepresenceApp.MainPage">

    <StackLayout>
        <Entry FontSize="20" Placeholder="Enter your User Id" x:Name="txtMyUser"/>

        <Entry FontSize="20" Placeholder="Enter  User Id" x:Name="txtUser"/>
        <Button x:Name="callButton" Clicked="Button_Clicked" Text="Start Call"/>
        <Button Text="Call" Clicked="Button_Clicked_1"/>
    </StackLayout>

</ContentPage>
